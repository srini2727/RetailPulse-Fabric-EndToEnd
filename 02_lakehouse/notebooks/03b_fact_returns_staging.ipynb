spark.sql("""
CREATE OR REPLACE TABLE gold_fact_returns_staging AS
SELECT
    r.return_id,
    r.order_id,
    r.order_item_id,
    oi.product_id,
    CAST(date_format(r.return_ts, 'yyyyMMdd') AS INT) AS date_key,
    r.refund_amount,
    r.return_reason,
    r.return_status
FROM silver_returns_clean r
LEFT JOIN silver_order_items_clean oi
    ON r.order_item_id = oi.order_item_id
""")

print("âœ… gold_fact_returns_staging created successfully")

